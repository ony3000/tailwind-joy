{
  "name": "@ony3000/experimental-ui",
  "version": "0.0.0",
  "description": "Joy UI의 component를 Tailwind CSS 방식으로 구현한 React component library",
  "keywords": [
    "react",
    "react-component",
    "design-system",
    "mui",
    "material-ui",
    "joy-ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ony3000/experimental-ui.git"
  },
  "license": "MIT",
  "author": "Hyeonjong <nianelo4@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/demo-lib.js",
      "require": "./dist/demo-lib.umd.cjs"
    }
  },
  "main": "./dist/demo-lib.umd.cjs",
  "module": "./dist/demo-lib.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch --minify=false"
  },
  "devDependencies": {
    "typescript": "4.9.5",
