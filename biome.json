{
  "$schema": "https://biomejs.dev/schemas/1.8.0/schema.json",
  "formatter": {
    "enabled": false
  },
  "organizeImports": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true
    },
    "ignore": [
      ".cache/**",
      ".docusaurus/**",
      ".next/**",
      "build/**",
      "dist/**",
      "playwright-report/**",
      "test-results/**"
    ]
  }
}
