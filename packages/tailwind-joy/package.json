{
  "name": "tailwind-joy",
  "version": "0.2.0",
  "description": "React component library that styles the components of Joy UI in the Tailwind CSS way.",
  "keywords": [
    "react",
    "react-component",
    "design-system",
    "mui",
    "material-ui",
    "joy-ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ony3000/tailwind-joy.git"
  },
  "license": "MIT",
  "author": "Hyeonjong <nianelo4@gmail.com>",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "import": "./dist/components.js",
      "require": "./dist/components.cjs"
    },
    "./tw-extension": {
      "import": "./dist/tw-extension.js",
      "require": "./dist/tw-extension.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch --minify=false",
    "preview": "vite build --minify=false"
  },
  "dependencies": {
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "react-icons": "5.2.1",
    "tailwind-merge": "2.3.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@types/react": "18.2.79",
    "@types/uuid": "10.0.0",
    "@vitejs/plugin-react": "4.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.4",
    "typescript": "4.9.5",
    "vite": "5.0.11",
    "vite-plugin-dts": "3.7.0"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": ">=3.4"
  },
  "packageManager": "pnpm@8.5.1"
}
